# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

#### custom configs for gitignore

# Meta Data RubyMine
/.idea

# Database
/db/schema.rb
/config/database.yml

# RVM config file
/.rvmrc

# MAC OS X folder metadata
*.DS_Store

# Shards config file
/config/shards.yml

# Redis dump
/dump.rdb

# joe`s backups
*~

# Uploaded Files && Compiled Assets
/public/assets/
/public/uploads/
/public/tmp/

#Aptana project metadata
/.project

# Tests coverage
/coverage




# Ignore Ruby versions
.rbenv
.ruby-version

# rvm gemset name
.ruby-gemset
